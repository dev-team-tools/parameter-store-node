  "bugs": {
    "url": "https://github.com/dev-team-tools/parameter-store-node/issues"
  },
  "homepage": "https://github.com/dev-team-tools/parameter-store-node#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@dev-team-tool/json-2-properties": "^1.1.2"
