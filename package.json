{
  "name": "@dev-team-tool/parameter-store-node",
  "version": "0.0.2",
  "description": "Library to access parameter-store from dev-team-tools.com programatically",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
